{
  "name": "",
  "version": "0.0.1-alpha",
  "description": "",
  "scripts": {
    "clean": "rimraf ./dist",
    "strip": "rimraf ./node_modules",
    "dev": "npm-run-all -p _dev:file-server _dev",
    "dev:auto-reload": "npm-run-all -p _dev:file-server _dev:auto-reload",
    "_dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --open --config webpack.dev.js",
    "_dev:auto-reload": "cross-env NODE_ENV=dev-auto-reload webpack-dev-server --progress --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cp -R ./src/webpage/static ./dist/",
    "_dev:file-server": "node ./build-utils/dev-file-server.js",
    "dist:server": "http-server ./dist -o"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "23.3.10",
    "@types/jss": "9.5.7",
    "@types/node": "10.12.11",
    "@types/react": "16.7.11",
    "chokidar": "2.0.4",
    "clean-webpack-plugin": "1.0.0",
    "cors": "^2.8.5",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "express": "^4.16.4",
    "fork-ts-checker-webpack-plugin": "0.5.0",
    "glob": "7.1.3",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "install": "^0.12.2",
    "jest": "23.6.0",
    "npm": "^6.4.1",
    "npm-run-all": "4.1.5",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "serve-index": "^1.9.1",
    "shelljs": "0.8.3",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.1",
    "ts-node": "7.0.1",
    "ts-shader-loader": "1.0.5",
    "tsconfig-paths": "3.7.0",
    "typescript": "3.2.1",
    "uglify-js": "3.4.9",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {
    "@material-ui/core": "3.6.1",
    "@material-ui/icons": "^3.0.1",
    "input-senders": "^1.0.2",
    "material-ui": "^1.0.0-beta.42",
    "orbit-controls": "1.2.4",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-router-dom": "4.3.1",
    "sanctuary": "^0.15.0"
  }
}
