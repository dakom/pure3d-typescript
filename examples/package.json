{
  "name": "",
  "version": "0.0.1-alpha",
  "description": "",
  "scripts": {
    "clean": "rimraf ./dist",
    "strip": "rimraf ./node_modules",
    "dev": "npm-run-all -p _dev:file-server _dev",
    "dev:auto-reload": "npm-run-all -p _dev:file-server _dev:auto-reload",
    "_dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --open --config webpack.dev.js",
    "_dev:auto-reload": "cross-env NODE_ENV=dev-auto-reload webpack-dev-server --progress --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cp -R ./src/webpage/static ./dist/",
    "_dev:file-server": "node ./build-utils/dev-file-server.js",
    "dist:server": "http-server ./dist -o"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@types/node": "10.0.0",
    "@types/react": "16.3.13",
    "chokidar": "2.0.3",
    "clean-webpack-plugin": "0.1.19",
    "cors": "^2.8.4",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "express": "^4.16.3",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "install": "^0.11.0",
    "jest": "22.4.3",
    "npm": "^6.0.0",
    "npm-run-all": "4.1.2",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "serve-index": "^1.9.1",
    "shelljs": "0.8.1",
    "source-map-loader": "0.2.3",
    "style-loader": "0.21.0",
    "ts-jest": "22.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "6.0.2",
    "ts-shader-loader": "1.0.5",
    "tsconfig-paths": "3.3.1",
    "typescript": "2.8.3",
    "uglify-js": "3.3.23",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2"
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "input-sender": "0.0.5-alpha",
    "material-ui": "^1.0.0-beta.42",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-router-dom": "4.2.2",
    "sodiumjs": "1.1.3"
  }
}
